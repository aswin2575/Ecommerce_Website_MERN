
    <section>
        <div class="container mt-5">
            <div class="row">
                {{#each products}}
                <div class="col-md-3 p-3">
                    <div class="card " style="width: 15rem; height: 24rem">
                        <img src="/product-images/{{this.item}}.jpg" class="card-img-top center-image " style="height: 12rem" alt="...">
                        <div class="card-body">
                            <h5 class="card-title">{{this.Name}}</h5>
                            <p class="card-text">Rs.{{this.product.Price}}</p>
                            <p class="card-text">Quantity: {{this.quantity}}</p>
                            
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </section>
    